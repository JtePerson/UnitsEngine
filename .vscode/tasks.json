{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cmake",
      "label": "CMake: configure",
      "command": "configure",
      "preset": "${command:cmake.activeConfigurePresetName}",
      "group": {
        "kind": "none",
        "isDefault": true
      },
      "problemMatcher": [],
      "detail": "CMake configure task"
    },
    {
      "type": "cmake",
      "label": "CMake: build",
      "command": "build",
      "targets": [
        "all"
      ],
      "preset": "${command:cmake.activeBuildPresetName}",
      "dependsOn": [
        "CMake: configure"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [],
      "detail": "CMake build task"
    }
  ]
}